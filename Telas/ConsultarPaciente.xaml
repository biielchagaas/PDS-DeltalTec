<Page x:Class="Projeto_DentalTec.Telas.ConsultarPaciente"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Projeto_DentalTec.Telas"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ConsultarPaciente">

    <Grid Background="#F9F7F7">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="3*"/>
        </Grid.ColumnDefinitions>

        <Border Grid.Column="0" Background="#3F72AF" BorderThickness="0"/>

        <Grid Grid.Column="0">

            <StackPanel Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center">
                <Image Source="/ImagensLogin/Logo.png" Height="95"/>
                <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
                    <Image Source="/Imagens/user.png"/>
                    <TextBlock Text="Funcionario"/>

                </StackPanel>
            </StackPanel>
            
        </Grid>
    </Grid>
</Page>
